//<Switch teams
FUNC {Func_SwitchTeamsPulse}:
	ScTA RRs_g Switch to green
	ScTA RRs_b Switch to blue
	J> RRs_b @a[team=RRg]
	J> RRs_g @a[team=RRb]
	E> RRg
	E> RRb
	J> RRb @a[team=RRs_b]
	J> RRg @a[team=RRs_g]
	clear @a[m=2,RRpi>=0]
	effect @a[m=2,RRpi>=0] clear
	ScTR RRs_g
	ScTR RRs_b
//>


//<Portal Start
FUNC {Func_PortalStart}:
	ARRAY {GLOBAL ACTIVATE CCU.objectiveAdd(Obj_PortalStart) Obj_PortalStart}:
		RRdi dummy Players
		RRdc dummy Display Calc
		RRsl dummy Player Select
	
	ScSS RRdi
	
	Green RRdi + 0
	Blue RRdi + 0
	J> Rg Green
	J> RRb Blue
