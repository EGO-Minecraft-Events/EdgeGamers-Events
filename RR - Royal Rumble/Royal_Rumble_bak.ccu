	CCU.spawnIDSetting()
	Func_GeneralScoreboardClock()
	Func_ControlPointClock()
	Func_StealthClock()
	Func_WarriorClock()
	Func_ArcherClock()
	Func_EngineerClock()
	Func_TeleporterClock()
	Func_GeneralEndClock()

GROUP {Grp_MainEnd}:
	CCU.spawnStandEnd()
	CCU_Deactivate(GSELF)
	Func_GeneralScoreboardEnd()
	Func_ControlPointEnd()
	Func_StealthEnd()
	Func_WarriorEnd()
	Func_ArcherEnd()
	Func_EngineerEnd()
	Func_TeleporterEnd()
	CCU_Deactivate(Grp_MainClock)
	CCU_Deactivate(Grp_ParticleTFClock)
	CCU_Deactivate(Grp_SimpleParticleTFClock)
	CCU_Deactivate(Grp_EngineerPigAstheticsClock)
	CCU_Deactivate(Grp_StealthInventoryRestoreTFClock)
	CCU_Deactivate(Grp_CountdownClock)
	CCU_Deactivate(Grp_ArrowproofGreenSpawnClock)
	CCU_Deactivate(Grp_ArrowproofBlueSpawnClock)
	CCU_Deactivate(Grp_ArrowproofBlueMansionClock)
	CCU_Deactivate(Grp_PortalClock)
	CCU_Deactivate(Grp_StealthGiveTFClock)
	CCU_Deactivate(Grp_WarriorGiveTFClock)
	CCU_Deactivate(Grp_ArcherGiveTFClock)
	CCU_Deactivate(Grp_EngineerGiveTFClock)

LOOP {Stealth;Warrior;Archer;Engineer} {0;1;2;3}:
	GROUP {Grp_|0|GiveTFClock}:
		testfor @a[$SA$,RRTarget|1|]
		CCU_SetblockTestfor
		
		Func_UniversalGive(RRTarget|1|)
		
		IF {|0| = Stealth}:
			Func_StealthGive
		IF {|0| != Stealth}:
			Func_StealthCC(RRTarget|1|)
		
		IF {|0| = Warrior}:
			Func_WarriorGive
		IF {|0| != Warrior}:
			Func_WarriorCC(RRTarget|1|)
		
		IF {|0| = Archer}:
			Func_ArcherGive
		IF {|0| != Archer}:
			Func_ArcherCC(RRTarget|1|)
		
		IF {|0| = Engineer}:
			Func_EngineerGive
		IF {|0| != Engineer}:
			Func_EngineerCC(RRTarget|1|)
		
		@a[$SA$,RRTarget|1|] - RRTarget|1|


GROUP {Grp_SummonAstheticsPulse}:
	CCU_Deactivate(GSELF)
	Func_SummonAestheticsPulse

CCU.cmdSave()

