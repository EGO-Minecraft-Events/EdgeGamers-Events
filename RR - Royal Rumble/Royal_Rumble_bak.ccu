//<Universal Class Change
FUNC {Func_UniversalGive}:
///	Heals, clears
	clear @a[$SA$,m=2,|0|]
	effect @a[$SA$,m=2,|0|] clear
	effect @a[$SA$,m=2,|0|] 6 1 100 true
