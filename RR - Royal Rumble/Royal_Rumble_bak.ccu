///	@a[$SA$,m=2,RRci=3,RRxp<=-1] RRxp = 0
	ScOP @a[$SA$,m=2,RRxp>=0] RRxp *= 100 Number
	ScOP @a[$SA$,m=2,RRci>=1<=2] RRxp /= 2000 Number
