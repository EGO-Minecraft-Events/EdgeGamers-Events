	@a[$SA$,m=2,|0|] RRaf + 0
	@a[$SA$,m=2,|0|] RRpi + 0
	@a[$SA$,m=2,|0|] RRar + 0
	@a[$SA$,m=2,|0|] RRrg + 0
	@a[$SA$,m=2,|0|] RRao + 0
	@a[$SA$,m=2,|0|] RRue + 0
	@a[$SA$,m=2,|0|] RRte + 0
