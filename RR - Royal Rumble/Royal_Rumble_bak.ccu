GROUP {Grp_EngineerPigAstheticsClock}:
	Func_EngineerPigAstheticsClock

GROUP {Grp_MainStart}:
	CCU.spawnStandStart()
	CCU_Deactivate(GSELF)
	Func_GeneralScoreboardStart()
	Func_ControlPointStart()
	Func_StealthStart()
	Func_WarriorStart()
	Func_ArcherStart()
	Func_EngineerStart()
	Func_TeleporterStart()
	CCU_Activate(Grp_MainClock)
	CCU_Activate(Grp_ParticleTFClock)
	CCU_Activate(Grp_SimpleParticleTFClock)
	CCU_Activate(Grp_EngineerPigAstheticsClock)
	CCU_Activate(Grp_StealthInventoryRestoreTFClock)
	CCU_Activate(Grp_StealthGiveTFClock)
	CCU_Activate(Grp_WarriorGiveTFClock)
	CCU_Activate(Grp_ArcherGiveTFClock)
	CCU_Activate(Grp_EngineerGiveTFClock)
	CCU_FinalSpawnStand

