	@e[type=ArmorStand,RRCountdown,RRti>=0<=19,RRti2>=1] RRti + 1
	@e[type=ArmorStand,RRCountdown,RRti=20,RRti2>=1] RRti2 - 1
	ScOP Countdown RR = @e[type=ArmorStand,RRCountdown,RRti=20] RRti2
