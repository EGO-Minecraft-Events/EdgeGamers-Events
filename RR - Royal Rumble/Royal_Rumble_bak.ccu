	ScOP @a[$SA$,m=2,RRci=0,RRxp>=0,RRrs=-1] RRxp /= $StealthCloakMax$ Number
	
	xp 1129L @a[$SA$,m=2,RRxp>=0]
	xp 11 @a[$SA$,m=2,RRxp>=0]
	LOOP {64 1 / 2} {6336 99 / 2}:
		xp |1| @a[$SA$,m=2,RRxp>=|0|]
		@a[$SA$,m=2,RRxp>=|0|] RRxp - |0|
	
	xp -1129L @a[$SA$,m=2,RRxp>=0]
