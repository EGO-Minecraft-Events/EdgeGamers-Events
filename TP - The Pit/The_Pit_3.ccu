//<Mobs General Clock
FUNC Func_MobsGeneralClock
	ScOP @e[type=ArmorStand,TPStand] TPmbc = @e[type=ArmorStand,TPStand] TPmb
	ScOP @e[type=ArmorStand,TPStand] TPmb = Mob_Mode TPMobSave
	ScOP @e[type=ArmorStand,TPStand] TPmbc -= @e[type=ArmorStand,TPStand] TPmb
	@e[type=ArmorStand,TPStand,TPmbc<=-1] TPmbs = 1
	@e[type=ArmorStand,TPStand,TPmbc>=1] TPmbs = 1
END
