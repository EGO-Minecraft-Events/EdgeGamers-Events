	@e[type=ArmorStand,TPStand,TPmbs>=1,TPmb=1] L> @a[TPsa=1,m=2,TPpl=1]
	@e[type=ArmorStand,TPStand,TPmbs>=1,TPmb=2] J> TPp @a[TPsa=1,m=2,TPpl=1,team=!TPp]
