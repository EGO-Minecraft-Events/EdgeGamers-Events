//<Countdown Clock
FUNC Func_CountdownClock
	@e[type=AreaEffectCloud,ST|2|CD] ST|2|ti + 1
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=20] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1]
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=20] tellraw @a[$SA$] $TextStart${"text":"5","color":"yellow","bold":"true"}]}
	
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=40] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1]
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=40] tellraw @a[$SA$] $TextStart${"text":"4","color":"yellow","bold":"true"}]}
	
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=60] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1]
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=60] tellraw @a[$SA$] $TextStart${"text":"3","color":"yellow","bold":"true"}]}
	
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=80] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1]
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=80] tellraw @a[$SA$] $TextStart${"text":"2","color":"yellow","bold":"true"}]}
	
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=100] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1]
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=100] tellraw @a[$SA$] $TextStart${"text":"1","color":"yellow","bold":"true"}]}
	
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=120] @a[$SA$] playsound minecraft:block.note.pling voice @a[c=1] ~ ~ ~ 2 2
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=120] tellraw @a[$SA$] $TextStart${"text":"GO!","color":"green","bold":"true"}]}
	@e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=120] @e[type=ArmorStand,tag=ST|2|AS] ST|2|ti = 0
	testfor @e[type=AreaEffectCloud,ST|2|CD,ST|2|ti=120]
	CONDITION
		Deactivate(CountdownClock|2|)
END
