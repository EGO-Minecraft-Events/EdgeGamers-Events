//<General Scoreboard Clock
FUNC Func_GeneralScoreboardClock
///	General
	$s$clear @a[$SA$,m=2,ST|2|pl>=0] sand
	effect @a[$SpawnArea$,m=2] 23 2 0 true
	$s$kill @e[$SA|2|$,type=Item]
	
///	Both Arenas Check
	@e[type=ArmorStand,STAS] STba = 0
	@e[type=ArmorStand,STAS] @e[type=ArmorStand,STAS] STba + 1

///	Sets sand
	@e[type=ArmorStand,ST|2|AS] STSave = 0
	ScOP @e[type=ArmorStand,ST|2|AS] STSave = Delay|2| STSave
	ScOP @e[type=ArmorStand,ST|2|AS] STSave *= 20 Number
	@e[type=ArmorStand,ST|2|AS,ST|2|ti>=1] ST|2|ti - 1
	@e[type=ArmorStand,ST|2|AS,ST|2|ti=0] @a[$SA|2|$] playsound minecraft:entity.experience_orb.touch voice @a[c=1] ~ ~ ~ 0.5
	@e[type=ArmorStand,ST|2|AS,ST|2|ti=0] fill $SBlockArea|2|$ redstone_block 0
	@e[type=ArmorStand,ST|2|AS,ST|2|ti=0] fill $SBlockArea|2|$ air 0
	ScOP @e[type=ArmorStand,ST|2|AS,ST|2|ti=0] ST|2|ti = @e[type=ArmorStand,ST|2|AS,ST|2|ti=0] STSave

///	Gives players resistance and fire resistance in the preselect region
	effect @a[$WinPreSelect|2|$,m=2,ST|2|pl>=0] 12 15 0 true
	effect @a[$WinPreSelect|2|$,m=2,ST|2|pl>=0] 11 2 10 true
	
///	Scoreboard display
	@e[type=ArmorStand,ST|2|AS,ST|2|as=0] @a[$WinSelect|2|$,ST|2|pl=0,c=1] STdi = -|4|
	@a[ST|2|pl=0,STdi=-|4|] (|2|_1st_place) STdi = -|4|
	@a[ST|2|pl=0,STdi=-|4|] @e[type=ArmorStand,ST|2|AS,ST|2|as=0] ST|2|as = 1
	@a[ST|2|pl=0,STdi=-|4|] tellraw @a[$SA$] $TextStart${"selector":"@a[r=0,c=1,ST|2|pl=0,STdi=-|4|]"},{"text":" got 1st place!","color":"green"}]}
	@a[ST|2|pl=0,STdi=-|4|] ST|2|pl = 1

	@e[type=ArmorStand,ST|2|AS,ST|2|as=1] @a[$WinSelect|2|$,ST|2|pl=0,c=1] STdi = -ADD(|4|;1)
	@a[ST|2|pl=0,STdi=-ADD(|4|;1)] (|2|_2nd_place) STdi = -ADD(|4|;1)
	@a[ST|2|pl=0,STdi=-ADD(|4|;1)] @e[type=ArmorStand,ST|2|AS,ST|2|as=1] ST|2|as = 2
	@a[ST|2|pl=0,STdi=-ADD(|4|;1)] tellraw @a[$SA$] $TextStart${"selector":"@a[r=0,c=1,ST|2|pl=0,STdi=-ADD(|4|;1)]"},{"text":" got 2nd place!","color":"green"}]}
	@a[ST|2|pl=0,STdi=-ADD(|4|;1)] ST|2|pl = 1
	
	@e[type=ArmorStand,ST|2|AS,ST|2|as=2] @a[$WinSelect|2|$,ST|2|pl=0,c=1] STdi = -ADD(|4|;2)
	@a[ST|2|pl=0,STdi=-ADD(|4|;2)] (|2|_3rd_place) STdi = -ADD(|4|;2)
	@a[ST|2|pl=0,STdi=-ADD(|4|;2)] @e[type=ArmorStand,ST|2|AS,ST|2|as=2] ST|2|as = 3
	@a[ST|2|pl=0,STdi=-ADD(|4|;2)] tellraw @a[$SA$] $TextStart${"selector":"@a[r=0,c=1,ST|2|pl=0,STdi=-ADD(|4|;2)]"},{"text":" got 3rd place!","color":"green"}]}
	@a[ST|2|pl=0,STdi=-ADD(|4|;2)] ST|2|pl = 1

///	@e[type=ArmorStand,ST|3|AS,ST|3|as>=1,STba=2] @e[type=ArmorStand,ST|2|AS,ST|2|as>=1,STba=2] say AYY LMAO DOES THIS WORK
	@e[type=ArmorStand,ST|3|AS,ST|3|as>=1,STba=2] @e[type=ArmorStand,ST|2|AS,ST|2|as>=1,STba=2] scoreboard players set'  'STdi -4

///	Teleport after scoreboard display
	$s$tp @a[$WinSelect|2|$,m=2] $SpawnCoords$
	$s$clear @a[$SpawnArea$,m=2] CCItem_Potion(-1;-1)
	ScTJ NoPVP @a[$SpawnArea$,m=2,team=!NoPVP]
	
	@a[$SpawnArea$,m=2,ST|2|pl=1] summon $Firework$
	@a[$SpawnArea$,m=2,ST|2|pl=1] ST|2|pl = 2

///	Suffocation
	@a[$SA|2|$,m=2,STki>=0] sand 0 ~ ~1 ~ @a[r=0,c=1] STki + 1
	@a[$SA|2|$,m=2,STki>=0] air 0 ~ ~2 ~ @a[r=0,c=1] STki = 0
	$s$kill @a[$SA|2|$,m=2,STki>=100]
	
///	Player Counter Display
	@e[type=ArmorStand,ST|2|AS] ST|2|pl = 0
	@a[$SA|2|$,m=2,ST|2|pl=0] @e[type=ArmorStand,ST|2|AS] ST|2|pl + 1
	ScOP |2|Players: STdi = @e[type=ArmorStand,ST|2|AS] ST|2|pl

/// Reset on death
	ScTL ST|2|p @a[ST|2|de>=1,ST|2|pl=0]
	@a[ST|2|de>=1,ST|2|pl=0,EC=0] SPbk = $SPid$
	ScRE @a[ST|2|de>=1] ST|2|pl
	ScRE @a[ST|2|de>=1] STki
	ScRE @a[ST|2|de>=1] ST|2|de

/// Options
///	Jump boost
	ScTE JumpBoost|2| STSave 1 1
	CONDITION
		effect @a[$SA|2|$,m=2,ST|2|pl=0] 8 2 1 true
	
///	Blindness
	ScTE Blindness|2| STSave 1 1
	CONDITION
		effect @a[$SA|2|$,m=2,ST|2|pl=0] 15 2 0 true
	
///	No Sprint
	ScTE NoSprint|2| STSave 1 1
	CONSTART
		effect @a[$SA|2|$,m=2,ST|2|pl=0] 17 2 127 true
		@a[$SA|2|$,m=2,ST|2|pl=0] ST|2|ti + 1
		effect @a[$SA|2|$,m=2,ST|2|pl=0,ST|2|ti>=30] 23 1 0 true
		@a[$SA|2|$,m=2,ST|2|pl=0,ST|2|ti>=30] ST|2|ti = 0
	CONEND
	effect @a[$SA|2|$,m=2,ST|2|pl=0,ST|2|ti>=20] 23 0 0 true
	
	ScTE NoSprint|2| STSave 0 0
	CONSTART
		effect @a[$SA|2|$,m=2,ST|2|pl=0] 23 2 0 true
		ScRE @a[$SA|2|$,m=2,ST|2|pl=0,ST|2|ti>=0] ST|2|ti
		effect @a[$SA|2|$,m=2,ST|2|pl=0] 17 0 0 true
	CONEND
	
///	Potions
	ScTE Potions|2| STSave 2 2
	CONDITION
		$s$clear @a[$SA|2|$,m=2,ST|2|pl=0] CCItem_Potion(-1;-1)
	
	ScTE Potions|2| STSave 2 2
	CONSTART
		Potions|2| STSave = 3
		$s$give @a[$SA|2|$,m=2,ST|2|pl=0] CCItem_Potion(1;0)
	CONEND
	
	//=
	ScTE Potions|2| STSave 0 0
	CONSTART
		effect @a[$SA|2|$,m=2,ST|2|pl=0,m=2] 8 0 0 true
	CONEND
	\\=
	
	ScTE Potions|2| STSave 0 0
	CONSTART
		Potions|2| STSave = 1
		$s$clear @a[$SA|2|$,m=2,ST|2|pl=0] CCItem_Potion(-1;-1)
	CONEND
	
///	Collisions
	ScTE Collisions|2| STSave 2 2
	CONSTART
		Collisions|2| STSave = 3
		scoreboard teams option ST|2|p collisionRule pushOtherTeams
	CONEND
	
	ScTE Collisions|2| STSave 0 0
	CONSTART
		Collisions|2| STSave = 1
		scoreboard teams option ST|2|p collisionRule never
	CONEND
END
//>

