//<Portal Start
FUNC Func_PortalStart
	USE $ObjA$
		ST dummy Sand Tomb
		STcl dummy Sand Tomb Calc
	UNUSE
	
	USE $TeamA$
		STd_dg
		STd_g
	UNUSE
	
	USE $TeamO$
		STd_dg color dark_green
		STd_g color green
	UNUSE
	
	scoreboard objectives setdisplay sidebar ST
	\/spawn ST = 1
	title @a[$SA$] title {"text":"|0| Arena","color":"green"}
	title @a[$SA$] subtitle {"text":"The door is now open","color":"green"}
	
	|1| ST = 0
	fill $TpDoor|2|$ air 0
	ScTJ STd_dg |1|
	ScTJ STd_g /spawn
	@a[$SA$] playsound minecraft:entity.experience_orb.touch voice @a[c=1]
	
	@e[type=ArmorStand,SpawnStand] SPtp = $SPid$
	
	BothArenas STcl = 0
	|2| STcl = 1
	ScTE |3| STcl 1 1
	CONDITION
		BothArenas STcl = 1
END
//>

//<Portal Clock
FUNC Func_PortalClock
	particle enchantmenttable $TpParticle|2|$ 0 0 0 1 4 force
	$s$gamemode 2 @a[$TpSelect|2|$,m=0]
	ScTG @a[$TpSelect|2|$,m=2] add STtp
	$s$tp @a[$TpSelect|2|$,m=2,STtp] $TpTo|2|$
	@a[$SA|2|$,m=2,STtp] playsound minecraft:entity.endermen.teleport voice @a[STtp,r=0,c=1]
	ScTG @a[$SA|2|$,m=2,STtp] remove STtp
END
//>

//<Portal End
FUNC Func_PortalEnd
	ScTE BothArenas STcl 0 0 
	CONSTART
		USE $ObjR$
			ST
			STcl
		UNUSE
		
		USE $TeamR$
			STd_dg
			STd_g
		UNUSE
		
		@e[type=ArmorStand,SpawnStand] SPtp = 0
		
		Activate(CBPPortalEnd|3|)
	CONEND
	
	ScRE |1| ST
	fill $TpDoor|2|$ stone_slab 1
	ScTG * remove STtp
	
	BothArenas STcl = 0
END
