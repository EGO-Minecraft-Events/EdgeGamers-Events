//<General Scoreboard End
FUNC Func_GeneralScoreboardEnd
	@e[type=ArmorStand,STAS] STba = 0
	@e[type=ArmorStand,STAS] @e[type=ArmorStand,STAS] STba + 1
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE |2|Players: STdi
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE (|2|_1st_place) STdi
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE (|2|_2nd_place) STdi
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE (|2|_3rd_place) STdi
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE  STdi
	@e[type=ArmorStand,STAS,STba=2,c=1] ScRE @a[score_STdi=-|4|,score_STdi_min=-ADD(|4|;2)] STdi
	
	testfor @e[type=ArmorStand,STAS,STba=1,c=1]
	CONDITION
		Activate(CBPMainEnd|3|)
	@e[type=ArmorStand,STAS,STba=1,c=1] $ObjR$ STdi
	@e[type=ArmorStand,STAS,STba=1,c=1] $ObjR$ STki
	@e[type=ArmorStand,STAS,STba=1,c=1] $ObjR$ STba

	USE $ObjR$
		ST|2|de
		ST|2|pl
		ST|2|as
		ST|2|ti
		ST|2|po
		ST|2|ju
	UNUSE
	
	USE $TeamR$
		ST|2|p
		ST|2|d_y
		ST|2|d_g
	UNUSE
	$s$kill @e[ST|2|]
	
	$s$kill @e[$SA|2|$,type=FallingSand]
	fill $SandArea|2|$ air 0
	
	$s$clear @a[$SA$,m=2]
	@a[$SA$,m=2,EC=0] SPbk = $SPid$
	effect @a[$SA$,m=2] clear
	$s$tp @a[$SA|2|$,m=2] -250 25 18 -90 0
	effect @a[$SA$] 10 10 100 true
	ScTG * remove STtp
END
