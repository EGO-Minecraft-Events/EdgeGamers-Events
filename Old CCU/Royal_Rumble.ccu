	@a[$SA$,m=2,RRgi=0] RRci = 0
	@a[$SA$,m=2,RRgi=0] RRur = 0
	@a[$SA$,m=2,RRgi=0] RRup = 0
	@a[$SA$,m=2,RRgi=0] RRuk = 0
	@a[$SA$,m=2,RRgi=0] RRsst = 0
