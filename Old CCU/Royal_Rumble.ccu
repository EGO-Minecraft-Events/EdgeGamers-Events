//<Arrowproof Green Spawn Testfor Clock
FUNC {Func_ArrowproofGreenSpawnTFClock}:
	testfor @e[type=ArmorStand,RRStand,RRap1=1] 
	CCU_SetblockTestfor
	
	LOOP {0 Arr_GreenSpawnArrow[-1] + 1}:
		LOOP {0 Arr_GreenSpawnArrow[|0|][-1] + 1}:
			IF {|0| != Arr_GreenSpawnArrow[|0;1|][-1]}:
				particle happyVillager Arr_GreenSpawnArrow[|0;1|][|0|] force @a
				
			IF {|0| = Arr_GreenSpawnArrow[|0;1|][-1]}:
				@e[Arr_GreenSpawnArrow[|0;1|][|0|],type=Arrow,RRate=2] RRka = 2
//>

