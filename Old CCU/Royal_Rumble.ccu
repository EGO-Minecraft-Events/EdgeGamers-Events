//<Engineer Clock
FUNC {Func_EngineerClock}:
//=
	Given 3 states:
	0: regular, can build
	1: when built, between 0 and 2 (giving carrot on a stick)
	2: when built
	
	Newly spawned, use RRpi as pig ID although not for pig, but for armor stand - Armorstand stores all info, while pig just has the display. If pig dies, is detected by the armor stand.
	Requires pig to be spawned not ontop of another pig for proper armor stand detection
	Requires baby pig detection and recovery
	Send requests via carrot on a stick. 1 request per tick will be processed

	Pig RRens:
	RRens 0: just spawned
	RRens 1: set after rotation
	RRens 2: set when on ground
	RRens 3: after all calculations on ground, ending
		
	Armor Stand RRens:
	RRens 0: just spawned
	RRens 2: immediately sets to 2 to avoid confusion as armor stands are summoned at RRens 2 stage of the pig
	RRens 3: ending
=//

///	Tags for the player when holding certain weapons
	@a[RRsa=1,m=2,RRci=3] RRhi = 30 {Inventory:[{Slot:-106b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Pickup"}}}]}
	@a[RRsa=1,m=2,RRci=3] RRhi = 31 {Inventory:[{Slot:-106b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Self Destruct"}}}]}
	@a[RRsa=1,m=2,RRci=3] RRhi = 32 {Inventory:[{Slot:-106b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Camp"}}}]}
	@a[RRsa=1,m=2,RRci=3] RRhi = 30 {SelectedItem:{id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Pickup"}}}}
	@a[RRsa=1,m=2,RRci=3] RRhi = 31 {SelectedItem:{id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Self Destruct"}}}}
	@a[RRsa=1,m=2,RRci=3] RRhi = 32 {SelectedItem:{id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Camp"}}}}
	
///	Toggles camp
///	Only works when you're on the ground to prevent abusing the ability to no fall damage
	@a[RRsa=1,m=2,RRci=3,RRcs>=1,RRhi=32,RRect=0] RRect = 1 {OnGround:1b}
	@a[RRsa=1,m=2,RRci=3,RRcs>=1,RRhi=32,RRect=2] RRect = 3
	@a[RRsa=1,m=2,RRci=3,RRect=2] RRect = 3 {OnGround:0b}
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRect=1,team=RRg] CCItem_EngieHeadResG
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRect=1,team=RRb] CCItem_EngieHeadResB
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRect=3,team=RRg] CCItem_EngieHeadG
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRect=3,team=RRb] CCItem_EngieHeadB
	@a[RRsa=1,m=2,RRci=3,RRect=1] RRect = 2
	@a[RRsa=1,m=2,RRci=3,RRect=3] RRect = 0
	
///	Effects for camp
	effect @a[RRsa=1,m=2,RRci=3,RRect=2] 2 5 10 true
	effect @a[RRsa=1,m=2,RRci=3,RRect=2] 8 5 250 true
	effect @a[RRsa=1,m=2,RRci=3,RRect=0] 2 0 0 true
	effect @a[RRsa=1,m=2,RRci=3,RRect=0] 8 0 0 true
	
///	Setting up the dispenser
	@a[RRsa=1,m=2,RRci=3] RRept + 0
	@e[type=Pig,RRPig] RRens + 0
	J> RRd_b @e[type=Pig,RRens=0,RRPigBlue]
	J> RRd_g @e[type=Pig,RRens=0,RRPigGreen]
	
///	Rotation of the pig
	@a[RRsa=1,m=2,RRci=3,RRept=0,RRue=1,ry=224,rym=135] @e[type=Pig,RRPig,RRens=0,r=6,c=1] + RRPigRotationS
	@a[RRsa=1,m=2,RRci=3,RRept=0,RRue=1,ry=44,rym=-45] @e[type=Pig,RRPig,RRens=0,r=6,c=1] + RRPigRotationN
	@a[RRsa=1,m=2,RRci=3,RRept=0,RRue=1,ry=-44,rym=-135] @e[type=Pig,RRPig,RRens=0,r=6,c=1] + RRPigRotationW
	@a[RRsa=1,m=2,RRci=3,RRept=0,RRue=1,ry=134,rym=45] @e[type=Pig,RRPig,RRens=0,r=6,c=1] + RRPigRotationE
	
///	Setting the PID of the pig
	@e[type=Pig,RRPigBlue,RRens=0] ScOP @e[c=1,r=1,type=Pig,RRPigBlue,RRens=0] RRpi = @a[c=1,r=6,team=RRb,RRci=3,RRept=0,RRue>=1] RRpi
	@e[type=Pig,RRPigGreen,RRens=0] ScOP @e[c=1,r=1,type=Pig,RRPigGreen,RRens=0] RRpi = @a[c=1,r=6,team=RRg,RRci=3,RRept=0,RRue>=1] RRpi
	@a[RRsa=1,m=2,RRci=3,RRue>=1] RRue = 2
	@e[type=Pig,RRens=0,RRPig] RRpi + 0
	kill @e[type=Pig,RRens=0,RRpi=0,RRPig]
	@e[type=Pig,RRens=0,RRPig] RRens = 1
	
///	Recovery for baby pig, with tellraw message
	@e[type=Pig] + RRPigBaby {CustomName:"Dispenser Pig"}
	@e[type=Pig,RRPigBaby] @a[r=6,RRci=3,RRept=0,RRue=0] + RRPlacedBabyTest
	@a[RRsa=1,m=2,RRci=3,RRPlacedBabyTest] minecraft:clear @a[r=1,c=1] spawn_egg -1 0
	@a[RRsa=1,RRci=3,RRept=0,RRaf=0,RRPlacedBabyTest] + RRPlacedBaby
	
///	Killing of baby pigs
	kill @e[type=Pig,RRPigBaby]
	@a[RRsa=1,m=2,RRci=3,RRPlacedBaby] + RRPigDispenserGive
	tellraw @a[RRsa=1,m=2,RRci=3,RRPlacedBaby] $TextStart${"text":"Don't place a pig on another pig!","color":"red","italic":true}]}
	@a[RRPlacedBabyTest] - RRPlacedBabyTest
	@a[RRPlacedBaby] - RRPlacedBaby
	
///	Objective: RRens
///	Tags for all: RRPigDispenser
///	Tags: RRPig, RRPigBlue, RRPigGreen
///	Tags for AS: RRASPig, RRASPigGreen, RRASPigBlue
	
///	OnGround set RRens to 2
	@e[type=Pig,RRens=1,RRPig] RRens = 2 {OnGround:1b}

///	Transfer from RRept 1 to RRept 2
	clear @a[RRsa=1,m=2,RRci=3,RRept=1] spawn_egg
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRept=1,!RRDispenserPickup] slot.hotbar.5 CCItem_EngiePickup
	@a[RRsa=1,m=2,RRci=3,RRept=1,RRDispenserPickup] RRepi = $EngiePickupDelay$
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRept=1] slot.hotbar.6 CCItem_EngieDestruct
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRept=1,RRDispenserPickup] slot.hotbar.1 CCItem_EngieBow
	@a[RRsa=1,m=2,RRci=3,RRept=1,RRDispenserPickup] - RRDispenserPickup
	@a[m=2,RRept=1,RRci=3] RRept = 2
	
///	Pickup CoS delay
	@a[RRsa=1,m=2,RRci=3,RRept=2,RRepi>=0] RRepi - 1
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRept=2,RRepi=0] slot.hotbar.5 CCItem_EngiePickup
	@a[RRsa=1,m=2,RRci=3,RRept=2,RRepi=0] playsound Snd_Item voice @a[c=1]
///	@a[RRepi=0] RRepi = -1
	@a[RRsa=1,m=2,RRepi>=1,RRept=0] RRepi = -1
	
///	Request for pickup, will test to see if the target matches
///	RRPickupRequest (person), RRPickupRequestTarget (armor stand)
///	Heads to the TF clock
	@a[RRsa=1,m=2,RRci=3,RRept=2,RRcs>=1,RRhi=30] + RRPickupRequest
	
///	If dead during pickup phaze
	@a[RRDispenserPickup,RRde>=1,RRci=3] + RRDeadPigPlayer
	@a[RRDispenserPickup,RRde>=1,RRci=3] - RRDispenserPickup

///	Request for self destruct
///	Heads to the TF clock
	@a[RRsa=1,m=2,RRci=3,RRept=2,RRcs>=1,RRhi=31] + RRDestructRequest
	
	@a[RReam>=0<=$EngiePickupDeath$,m=2] RReam - 1
	@a[m=2,RRci=3,RReam=0,RRept=0] + RRPigDispenserGive
	@a[m=2,RRci=3,RReam=0,RRept=0] playsound Snd_Level voice @a[c=1]
	
///	Upon death of the dispenser
///	Heads over to the TF Clock
	@e[type=ArmorStand,RRASPig] RRedp = 1
	@e[type=Pig,RRPig] ~ ~-1 ~ @e[type=ArmorStand,RRASPig,dy=1] RRedp = 0
	
///	Gives back pig egg
	replaceitem entity @a[RRci=3,team=RRg,RRPigDispenserGive] slot.hotbar.4 CCItem_EngieEggG
	replaceitem entity @a[RRci=3,team=RRb,RRPigDispenserGive] slot.hotbar.4 CCItem_EngieEggB
	@a[RRPigDispenserGive] - RRPigDispenserGive
	
///	Ammo regeneration
///	Has to regen cloak, rockets and blindness arrows
	@a[RRsa=1,m=2] RRend = 0
	@e[type=Pig,RRPigGreen] @a[r=$DispenserRadius$,m=2,team=RRg] RRend = 1
	@e[type=Pig,RRPigBlue] @a[r=$DispenserRadius$,m=2,team=RRb] RRend = 1
	@a[RRsa=1,m=2,RRci>=2<=3,RRend=0] RRao = 0
	@a[RRsa=1,m=2,RRci=0,RRsst=0,RRsam>=-1<=279,RRend=1] RRsam + 1
	@a[RRsa=1,m=2,RRci=2,RRend=1] clear @a[r=0,c=1,RRci=2] CCItem_ArcherRocket(-1;0)
	@a[RRsa=1,m=2,RRci=3,RRend=1] clear @a[r=0,c=1,RRci=3] CCItem_EngieArrow(-1;0)
	
	@a[RRsa=1,m=2,RRci=2,RRaf<$ArcherRocketNum$,RRao<=99,RRend=1] RRao + 1
	replaceitem entity @a[RRsa=1,m=2,RRci=2,RRaf=0,RRao=$DispenserRocketDelay$,RRend=1] slot.hotbar.7 CCItem_ArcherRocket(1;0)
	give @a[RRsa=1,m=2,RRci=2,RRaf>=1<$ArcherRocketNum$,RRao=$DispenserRocketDelay$,RRend=1] CCItem_ArcherRocket(1;0)
	@a[RRsa=1,m=2,RRci=2,RRend=1,RRao>=$DispenserRocketDelay$] RRao = 0
	
	@a[RRsa=1,m=2,RRci=3,RRaf<=1,RRao<=149,RRend=1] RRao + 1
	replaceitem entity @a[RRsa=1,m=2,RRci=3,RRaf=0,RRao=$DispenserBlindnessDelay$,RRend=1] slot.hotbar.7 CCItem_EngieArrow(1;0)
	give @a[RRsa=1,m=2,RRci=3,RRaf=1,RRao=$DispenserBlindnessDelay$,RRend=1] CCItem_EngieArrow(1;0)
	@a[RRsa=1,m=2,RRci=3,RRend=1,RRao>=$DispenserBlindnessDelay$] RRao = 0

//=	Health regeneration
	Cannot use regeneration anywhere else
	0: reset
	1: engie regen
	2: dispenser regen (takes over engie regen ofc)
	Transfer from 2 to 1 and 1 to 2: does not reset timer
	RRert Regen State
=//

	effect @a[RRsa=1,m=2] 10 0 0 true
	
///	Regens faster when you've been near a dispenser for longer
	@a[RRsa=1,m=2,RRend=0,RRedt>0] RRedt - 1
	@a[RRsa=1,m=2,RRend=1,RRedt<$DispenserRegenDelayTime$] RRedt + 1
	
	@a[RRsa=1,m=2,RRci=3,team=RRg] @a[r=$EngieRegenRadius$,m=2,team=RRg] RRert = 1
	@a[RRsa=1,m=2,RRci=3,team=RRb] @a[r=$EngieRegenRadius$,m=2,team=RRb] RRert = 1
	@a[RRsa=1,m=2,RRend=1] RRert = 2
	@a[RRsa=1,m=2,RRert=0,RRrg>=1] RRrg = 0
	@a[RRsa=1,m=2,RRert>=1<=2,RRrg>=0<$EngieRegenDelay$] RRrg + 1
	effect @a[RRsa=1,m=2,RRert=1,RRrg=$EngieRegenDelay$] 10 1 10 true
	effect @a[RRsa=1,m=2,RRert=2,RRrg=$DispenserRegenDelay$] 10 1 10 true
	effect @a[RRsa=1,m=2,RRert=2,RRedt>=$DispenserRegenDelayTime$,RRrg=$DispenserRegenDelay2$] 10 1 10 true
	
	@a[RRsa=1,m=2,RRert=1,RRrg>=$EngieRegenDelay$] RRrg = 0
	@a[RRsa=1,m=2,RRert=2,RRrg>=$DispenserRegenDelay$] RRrg = 0
	@a[RRsa=1,m=2,RRert=2,RRedt>=$DispenserRegenDelayTime$,RRrg=$DispenserRegenDelay2$] RRrg = 0
	@a[RRert>=1] RRert = 0

///	XP Start Calc
	@a[RRsa=1,m=2,RRci=3,RRepi>=0] ScOP @a[m=2,RRci=3,RRepi>=0,c=1,r=1] RRxp = @a[m=2,RRci=3,RRepi>=0,c=1,r=1] RRepi
	@a[RRsa=1,m=2,RRci=3,RRepi=-1] ScOP @a[m=2,RRci=3,RRepi=-1,c=1,r=1] RRxp = @a[m=2,RRci=3,RRepi=-1,c=1,r=1] RReam
	
///XP Sound effects
///Plays every second
///	@a[RRsa=1,m=2,RRept=0,RRci=3,RReam>=1<=319] RRpcl4 = RReam
///	@a[RRsa=1,m=2,RRept=0,RRci=3,RReam>=1<=319] RRpcl4 % 20
///	@a[RRsa=1,m=2,RRept=0,RRci=3,RRpcl4=0,RReam>=1<=319] playsound entity.experience_orb.touch voice @a[c=1] ~ ~ ~ 0.5
///	@a[m=2,RRci=3,RRpcl4<=320] RRpcl4 = 0

///	Teleporting
///	RRetd
	@a[RRsa=1,m=2,RRci=3] + RREngineerTeleport {ActiveEffects:[{Id:13b,Amplifier:0b}]}
	@a[RRsa=1,m=2,RRci=3,RREngineerTeleport] RRetd + 0
	effect @a[RREngineerTeleport] 13 0 0 true
	@a[RREngineerTeleport] - RREngineerTeleport
//>

//<Engineer Pickup Request Testfor Clock
FUNC {Func_EngineerPickupRequestTFClock}:
	testfor @a[RRPickupRequest]
	CCU_SetblockTestfor
	
	@a[RRsa=1,RRPickupRequest] @e[type=ArmorStand,c=1,r=3,RRASPig] + RRPickupRequestTarget
	
	@a[RRsa=1,RRPickupRequest] ScOP @a[c=1,r=1,RRsa=1,RRPickupRequest] RRpcl1 = @a[c=1,r=1,RRsa=1,RRPickupRequest] RRpi
	@a[RRsa=1,RRPickupRequest] ScOP @a[c=1,r=1,RRsa=1,RRPickupRequest] RRpcl1 -= @e[type=ArmorStand,RRPickupRequestTarget,c=1,r=3] RRpi
	@a[RRsa=1,RRPickupRequest,RRpcl1=0] @e[type=ArmorStand,RRPickupRequestTarget,c=1,r=3] ~ ~1 ~ @e[type=Pig,RRPig,c=1,dy=-3] + RRPickupDeath
	@e[type=Pig,RRPickupDeath] particle explode ~ ~0.5 ~ 0.5 0.3 0.5 0 30 force
	@e[type=Pig,RRPickupDeath] playsound Snd_Item voice @a[r=5]
	tp @e[type=Pig,RRPickupDeath] ~ ~-150 ~
	kill @e[type=Pig,RRPickupDeath]
	@a[RRsa=1,RRPickupRequest,RRpcl1=0] kill @e[type=ArmorStand,RRPickupRequestTarget,c=1,r=3]
	
///	If RRpcl1 matches with a score of 0, activates all
	clear @a[RRsa=1,RRPickupRequest,RRpcl1=0] carrot_on_a_stick 0 -1 {ench:[{id:0,lvl:1}]}
	clear @a[RRsa=1,RRPickupRequest,RRpcl1=0] bow
	@a[RRsa=1,RRPickupRequest,RRpcl1=0] + RRPigDispenserGive
	@a[RRsa=1,RRPickupRequest,RRpcl1=0] + RRDispenserPickup
	@a[RRsa=1,m=2,RRDispenserPickup] + RRWeakness
	@a[RRsa=1,RRPickupRequest,RRpcl1=0] RRept = 0
	@a[RRPickupRequest,RRpcl1<=10000] - RRPickupRequest
	@a[RRpcl1<=10000] reset RRpcl1
	@e[type=ArmorStand,RRPickupRequestTarget] - RRPickupRequestTarget
//>

