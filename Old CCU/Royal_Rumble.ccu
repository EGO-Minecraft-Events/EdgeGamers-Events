//<Teleporter Start
///	Activated by tag "RRTPOn" done to the RRParticle armor stand. Open defined with "RRTPOpen".
///	Once activated, does playsound, tellraw, join team to blue, setblock glass
///	Avaliable Tags: RRTeleport#, RRParticle, RRTPOn, RRTPOpen, RRTeleporter, RRTeleport
///	Deactivated by RRTPOff

///	RRtpo 1: Initial enable
///	RRtpo 2: Final enable
///	RRtpo 3: Initial disable
///	RRtpo 0: Final disable

FUNC {Func_TeleporterStart}:
	ARRAY {GLOBAL ACTIVATE CCU.objectiveAdd(Obj_TeleporterStart) Obj_TeleporterStart}:
		RRpa dummy RR Particles
		
		RRtpi dummy RR Teleport ID
		RRtpi2 dummy RR Teleport ID 2
		RRtpo dummy RR Teleport Toggle
		RRpat dummy RR Particle Type
		RRttp dummy RR Teleport TP

///	Particles & Teleporter
	LOOP {0 Arr_TPParticle[-1] + 1} {1 Arr_TPParticle[L] + 1}:
		summon AreaEffectCloud Arr_TPTeleportFrom[|0|] {$Duration$,Tags:["RRTeleporter","RRTeleport","RRTeleport|1|"],CustomName:"Teleporter |1|"}
		summon AreaEffectCloud Arr_TPParticle[|0|] {$Duration$,Tags:["RRTeleporter","RRParticle","RRTeleport|1|"],CustomName:"Teleporter |1|"}
		@e[type=AreaEffectCloud,RRTeleport|1|] RRtpi = |1|
	
	@e[type=AreaEffectCloud,RRTeleporter] RRtpo + 0
	@e[type=AreaEffectCloud,RRTeleporter] RRpa + 0
	@e[type=AreaEffectCloud,RRTeleporter] RRpat = 0
	J> RRd_b @e[type=AreaEffectCloud,RRParticle]
//>

//<Teleporter Activation Testfor Clock
FUNC {Func_TeleporterActivationTFClock}:
	testfor @e[type=AreaEffectCloud,RRTeleporter,RRtpo=1]
	CCU_SetblockTestfor
	
///	Activation
	@e[type=AreaEffectCloud,RRParticle,RRtpo=1] playsound minecraft:block.lava.extinguish voice @a[team=RRg] ~ ~ ~ 2 0.5
	@e[type=AreaEffectCloud,RRParticle,RRtpo=1] tellraw @a $TextStart${"selector":"@e[type=AreaEffectCloud,RRParticle,RRtpo=1]"},{"text":" has been enabled!","color":"gray"}]}
		
	J> RRd_g @e[type=AreaEffectCloud,RRParticle,RRtpo=1,RRtpi>=1<=8]
	LOOP {1 (Arr_TPGlass[L] + 1) + 1} {Arr_TPGlass[S]}:
		@e[type=AreaEffectCloud,RRParticle,RRtpo=1,RRtpi=|0|] setblock |1| stained_glass 5
	
	@e[type=AreaEffectCloud,RRTeleporter,RRtpo=1] RRtpo = 2
//>

//<Teleporter Deactivation Testfor Clock
FUNC {Func_TeleporterDeactivationTFClock}:
	testfor @e[type=AreaEffectCloud,RRTeleporter,RRtpo=3]
	CCU_SetblockTestfor
	
///	Deactivation
	@e[type=AreaEffectCloud,RRParticle,RRtpo=3] playsound Snd_Item voice @a[r=10,team=RRg] ~ ~ ~ 2 0.5
	@e[type=AreaEffectCloud,RRParticle,RRtpo=3] tellraw @a $TextStart${"selector":"@e[c=1,r=1,type=AreaEffectCloud,RRParticle,RRtpo=3]"},{"text":" has been disabled!","color":"gray"}]}
	J> RRd_b @e[type=AreaEffectCloud,RRParticle,RRtpo=3,RRtpi>=1<=8]
	LOOP {1 (Arr_TPGlass[L] + 1) + 1} {Arr_TPGlass[S]}:
		@e[type=AreaEffectCloud,RRParticle,RRtpo=3,RRtpi=|0|] setblock |1| stained_glass 3
	
	@e[type=AreaEffectCloud,RRTeleporter,RRtpo=3] RRtpo = 0
//>

//<Teleporter Teleport Testfor Clock
FUNC {Func_TeleporterTeleportTFClock}:
	testfor @a[RRsa=1,m=2,RRpi>=1,RRttp>=1]
	CCU_SetblockTestfor
	
	@a[RRsa=1,m=2,RRpi>=1,team=RRg,RRttp>=1] RRte = 1
	effect @a[RRsa=1,m=2,RRpi>=1,team=RRg,RRttp>=1] 11 1 4 true
	@a[RRsa=1,m=2,RRpi>=1,team=RRg,RRttp>=1] RRre = 20
	
	@a[RRsa=1,m=2,RRpi>=1,team=RRb,RRttp>=1] RRte = 2
	effect @a[RRsa=1,m=2,RRpi>=1,team=RRb,RRttp>=1] 11 2 4 true
	@a[RRsa=1,m=2,RRpi>=1,team=RRb,RRttp>=1] RRre = 40
	
	LOOP {1 Arr_TPTeleportTo[L] + 1} {Arr_TPTeleportTo[S]}:
		tp @a[RRsa=1,m=2,RRpi>=1,RRttp=|0|] |1|
		
	@a[RRsa=1,m=2,RRpi>=1,RRttp>=1] RRttp = 0
//>

//<Teleporter End
FUNC {Func_TeleporterEnd}:
	CCU.objectiveRemove(Obj_TeleporterStart)
	
	kill @e[type=AreaEffectCloud,RRTeleporter]
