//<Engineer Pig Initialize Testfor Clock
FUNC {Func_EngineerPigInitializeTFClock}:
	testfor @e[type=Pig,RRPig,RRens=2]
	CCU_SetblockTestfor
	
///	Detection of a pig being on another pig
///	Closest player with RRue of 2 to pig with RRens of 2, RRae min of 2 and RRept of 0
	@e[type=Pig,RRPig,RRens=2] stats entity @e[RRPig,r=0,c=1] set AffectedEntities @e[RRPig,r=0,c=1] RRae
	@e[type=Pig,RRPig] RRae + 0
	@e[type=Pig,RRPig] tellraw @e[type=Pig,RRPig,r=0] baby i'm ballin
	
	@e[type=Pig,RRPig,RRae>=2,RRens=2] + RRPigPlaced
	@e[type=Pig,RRPigPlaced] @a[r=6,c=1,RRue>=2,RRept=0] + RRPlacedOnPig
	kill @e[type=Pig,RRPigPlaced]
	@a[RRsa=1,m=2,RRci=3,RRPlacedOnPig] + RRPigDispenserGive
	@a[RRsa=1,m=2,RRci=3,RRPlacedOnPig] RRue = 0
	tellraw @a[RRsa=1,m=2,RRci=3,RRPlacedOnPig] $TextStart${"text":"Don't place a pig on top of another pig!","color":"red","italic":true}]}
	@a[RRPlacedOnPig] - RRPlacedOnPig
	
	
///	Regular summoning of armor stands, removal of invulnerability from pigs
	@e[type=Pig,RRens=2,RRPig] RRaar + 0
	tp @e[type=Pig,RRPigRotationN,RRens=2] ~ ~ ~ 180 0
	tp @e[type=Pig,RRPigRotationS,RRens=2] ~ ~ ~ 0 0
	tp @e[type=Pig,RRPigRotationE,RRens=2] ~ ~ ~ 270 0
	tp @e[type=Pig,RRPigRotationW,RRens=2] ~ ~ ~ 90 0
	
	/*
	@e[type=Pig,RRens=2,RRPigGreen] summon ArmorStand ~ ~ ~ 
		{Tags:["RREntity","RRPigDispenser","RRASPig","RRASPigGreen"],
		DisabledSlots:2096896,Marker:1,NoGravity:1,NoBasePlate:1,ShowArms:1,
		Small:1,Invulnerable:1,PersistenceRequired:1,Invisible:1,
		ArmorItems:[{},{},{},{id:stained_hardened_clay,Count:1b,Damage:5s}]}
	
	-/-
	
	@e[type=Pig,RRens=2,RRPigBlue] summon ArmorStand ~ ~ ~ 
		{Tags:["RREntity","RRPigDispenser","RRASPig","RRASPigBlue"],
		DisabledSlots:2096896,Marker:1,NoGravity:1,NoBasePlate:1,ShowArms:1,
		Small:1,Invulnerable:1,PersistenceRequired:1,Invisible:1,
		ArmorItems:[{},{},{},{id:stained_hardened_clay,Count:1b,Damage:3s}]}
	
	*/
	
	@e[type=ArmorStand,RRASPig] RRens + 0
	@e[type=ArmorStand,RRASPig,RRens=0] RRens = 2
	entitydata @e[type=Pig,RRPig,RRens=2] {Invulnerable:0}
	@e[type=ArmorStand,RRASPig,RRens=2] ScOP @e[c=1,r=1,type=ArmorStand,RRASPig,RRens=2] RRpi = @e[c=1,dy=-1,type=Pig,RRPig,RRens=2] RRpi
	
///	Detection of RRept to go to 1
///	RRpcl5
	@e[type=ArmorStand,RRASPig,c=1,RRens=2] + RRASPigRRptChoose
	@e[type=ArmorStand,RRASPigRRptChoose] @a[m=2,RRci=3,RRept=0,RRue=2] ScOP @a[c=1,r=1,m=2,RRci=3,RRept=0,RRue=2] RRpcl5 = @a[c=1,r=1,m=2,RRci=3,RRept=0,RRue=2] RRpi
	ScOP @a[m=2,RRci=3,RRept=0,RRue=2] RRpcl5 -= @e[type=ArmorStand,RRASPigRRptChoose] RRpi
	@a[m=2,RRci=3,RRept=0,RRue=2,RRpcl5=0] RRept = 1
	
	@a[RRci=3,RRept=1,RRue>=2] RRue = 0
	@e[type=Pig,RRPig,RRens=2] RRens = 3
	@e[type=ArmorStand,RRASPigRRptChoose,RRens=2] RRens = 3
	@a[RRpcl5<=10000] reset RRpcl5
	@e[type=ArmorStand,RRASPigRRptChoose] - RRASPigRRptChoose
//>

