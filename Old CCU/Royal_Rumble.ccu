	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore] slot.inventory.26 CCItem_Book_Universal
	
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,team=RRg] CCItem_StealthHeadG
