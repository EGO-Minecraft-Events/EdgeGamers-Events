//<Archer Clock
FUNC {Func_ArcherClock}:
//=
	RRaar 0: adding
	RRaar 1: in air
	RRaar 2: in ground, adding
	RRaar 3: only goes to this score if it has the RRExplosiveArrow tag to prevent damage twice
	
	RRaar 1 for players: long range
	RRaar 2 for players: close range
	
	RRaid 1: Normal
	RRaid 2: Explosive
	RRaid 3: Blindness
	
	RRate 1: Green
	RRate 2: Blue
=//
	
///	Bow ID
///	1 is holding reg, while 2 is reinforced. reset at end. Replaces "RRHoldingExplosives tag"
	@a[RRsa=1,m=2,RRci=2] RRhi = 21 {Inventory:[{Slot:-106b,id:"minecraft:bow",Count:1b,tag:{display:{Name:"Regular bow"}}}]}
	@a[RRsa=1,m=2,RRci=2] RRhi = 22 {Inventory:[{Slot:-106b,id:"minecraft:bow",Count:1b,tag:{display:{Name:"Reinforced bow"}}}]}
	@a[RRsa=1,m=2,RRci=2] RRhi = 21 {SelectedItem:{id:"minecraft:bow",Count:1b,tag:{display:{Name:"Regular bow"}}}}
	@a[RRsa=1,m=2,RRci=2] RRhi = 22 {SelectedItem:{id:"minecraft:bow",Count:1b,tag:{display:{Name:"Reinforced bow"}}}}

///	Regular arrow
	replaceitem entity @a[RRsa=1,m=2,RRci=2,RRhi=21] slot.inventory.0 CCItem_ArcherArrow(1;0)
	minecraft:clear @a[RRsa=1,m=2,RRci=2,RRhi=22] CCItem_ArcherArrow(-1;-1)

///	Tags arrows
	@e[type=Arrow] RRaar + 0
	@e[type=Arrow] RRaid + 0
	@e[type=Arrow] RRate + 0
	@e[type=Arrow] RRka + 0
	
	@a[RRsa=1,m=2,RRci>=2<=3,RRub>=1,team=RRg] @e[type=Arrow,RRaar=0,r=5,c=1,RRate=0] RRate = 1
	@a[RRsa=1,m=2,RRci>=2<=3,RRub>=1,team=RRb] @e[type=Arrow,RRaar=0,r=5,c=1,RRate=0] RRate = 2
	
	@e[type=Arrow,RRaar=0,RRaid=0] RRaid = 3 {damage:4.0d}
	@a[RRsa=1,m=2,RRci=2,RRub>=1,RRhi=22] ScOP @e[type=Arrow,RRaar=0,r=5,c=1,RRaid=3] RRpi = @a[r=2,c=1,m=2,RRci=2,RRub>=1,RRhi=22] RRpi
	@e[type=Arrow,RRaar=0,RRaid=0] RRaid = 2 {CustomPotionEffects:[{Id:15b,Amplifier:0b,Duration:160}]}
	entitydata @e[type=Arrow,RRaar=0,RRaid=2] {damage:0.01d}
	@e[type=Arrow,RRaar=0,RRaid=0] RRaid = 1
	
	@e[type=Arrow,RRaar=0] RRaar = 1
	
///	Particle and playsound effects
	@e[type=Arrow,RRaar=1] RRaar = 2 {inGround:1b}
	
//=	Damage dealt to other team setting
	Requires both direct and splash damage
	Get averages
	
	Use RRaar on players: 1 for far, 2 for near, 0 for neutral
	radius of 2: near. 4: far
	All resistance effects cleared here: change if necessary
=//
	
	effect @a[RRsa=1,m=2,RRre=0] 11 0 0 true
	effect @e[type=Pig,RRPig] 11 0 0 true
	
	@a[RRub>=1] RRub = 0
	
///	Adds to kill arrows
	@e[type=Arrow,RRaar=2,RRaid>=1<=2] RRka = 1
//>

//<Archer Arrow Damage Testfor Clock
FUNC {Func_ArcherArrowDamageTestforClock}:
	testfor @e[type=Arrow,RRaar=2]
	CCU_SetblockTestfor
	
	@e[type=Arrow,RRka=0,RRaid=3,RRaar=2,c=1] RRaar = 3
	@e[type=Arrow,RRka=0,RRaid=3,RRaar=3] playsound Snd_Explode voice @a ~ ~ ~ 1 0.75
	@e[type=Arrow,RRka=0,RRaid=3,RRaar=3] particle hugeexplosion ~ ~ ~ 0 0 0 0 1 force
	
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=1] ~ ~-1 ~ @a[m=2,team=RRb,r=$ArcherExplosionFar$,RRaar=0] RRaar = 1
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=1] ~ ~-1 ~ @a[m=2,team=RRb,r=$ArcherExplosionNear$,RRaar=1] RRaar = 2
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=2] ~ ~-1 ~ @a[m=2,team=RRg,r=$ArcherExplosionFar$,RRaar=0] RRaar = 1
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=2] ~ ~-1 ~ @a[m=2,team=RRg,r=$ArcherExplosionNear$,RRaar=1] RRaar = 2
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=1] ~ ~-1 ~ @e[type=Pig,RRPigBlue,r=$ArcherExplosionFar$,RRaar=0] RRaar = 1
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=1] ~ ~-1 ~ @e[type=Pig,RRPigBlue,r=$ArcherExplosionNear$,RRaar=1] RRaar = 2
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=2] ~ ~-1 ~ @e[type=Pig,RRPigGreen,r=$ArcherExplosionFar$,RRaar=0] RRaar = 1
	@e[type=Arrow,RRka=0,RRaar=3,RRaid=3,RRate=2] ~ ~-1 ~ @e[type=Pig,RRPigGreen,r=$ArcherExplosionNear$,RRaar=1] RRaar = 2
	
	@e[type=Arrow,RRka=0,RRaar=3] ~ ~-1 ~ @a[m=2,RRci=2,r=$ArcherExplosionSelfFar$] RRach = 1
	@e[type=Arrow,RRka=0,RRaar=3] ~ ~-1 ~ @a[m=2,RRci=2,r=$ArcherExplosionSelfNear$] RRach = 2
	
///	Self inflicted damage setting
///	Use the pig pickup format but with chooses like self destuct
///	Also starts from an entity versus a player, which means all players - entity
	
	@a[m=2,RRci=2,RRaar=0,RRach>=1<=2] ScOP @a[c=1,r=1,m=2,RRci=2,RRaar=0,RRach>=1<=2] RRacl = @a[c=1,r=1,m=2,RRci=2,RRaar=0,RRach>=1<=2] RRpi
	ScOP @a[RRsa=1,m=2,RRci=2,RRach>=1<=2] RRacl -= @e[type=Arrow,c=1,RRka=0,RRaar=3] RRpi
	@a[m=2,RRci=2,RRacl=0,RRaar=0,RRach>=1<=2] ScOP @a[c=1,r=1,m=2,RRci=2,RRacl=0,RRaar=0,RRach>=1<=2] RRaar = @a[c=1,r=1,m=2,RRci=2,RRacl=0,RRaar=0,RRach>=1<=2] RRach
	
///	Reset all arrow scores and tags
	@a[RRacl<=10000] reset RRacl
	@a[RRach<=10000] reset RRach
	
//=	Deals damage based on class and range
		HrmLvl	ResLvl	Dmg		CN	CF
		1		2		1.8			
		1		1		2.4			
		1		0		3			ae
		2		2		3.6			
		2		1		4.8			sw
		2		0		6		swae	
	Pig = archer
=//

	effect @a[RRsa=1,m=2,RRci>=0<=1,RRaar=1] 11 1 0 true
	@a[RRsa=1,m=2,RRci>=0<=1,RRaar=1] RRre = 3
	
	effect @a[RRsa=1,m=2,RRci>=0<=1,RRaar=1] 7 1 1 true
	effect @a[RRsa=1,m=2,RRci>=2<=3,RRaar=1] 7 1 0 true
	effect @a[RRsa=1,m=2,RRci>=0<=4,RRaar=2] 7 1 1 true
	
	effect @e[type=Pig,RRaar=1,RRPig] 11 1 0 true
	effect @e[type=Pig,RRaar>=1<=2,RRPig] 7 1 0 true
	
	@e[type=Arrow,RRaar=3] RRka = 1
	
	@a[RRaar>=1] RRaar = 0
	@e[type=Pig,RRaar>=1] RRaar = 0
//>

