//<Stealth Start
FUNC {Func_StealthStart}:
	ARRAY {GLOBAL ACTIVATE CCU.objectiveAdd(Obj_StealthStart) Obj_StealthStart}:
		RRur stat.useItem.minecraft.redstone_block RR Used Redstone
		RRup stat.useItem.minecraft.lingering_potion RR Used Lingering Potion
		RRuk stat.useItem.minecraft.golden_sword RR Used Big Earner
		
		RRsam dummy RR Sneak Amount
		RRsst dummy RR Sneak State
		RRsed dummy RR Sneak End
		RRsis dummy RR Inventory Slot
		RRsia dummy RR Inventory Save
		RRsic dummy RR Inventory Calc
		RRsrt dummy RR Restock Timer
		RRspo dummy RR Thrown Potion
//>

//<Stealth Clock: Cloaking
FUNC {Func_StealthClock}:
//= Features
	-Change slot: uncloak
	-Currently lasts 7s
	-Currently takes 7s to recharge
	-Remembers if the player used the redstone block, knife and potion
	-Does not remember inventory layout
	-Takes 1s to decloak, with a notable noise + particle effect
		-1s does not count for increase or decrease of the cloak: it stays as is until fully uncloaked
		-Only given items after that 1s
	-Cannot attack while cloaked, but can be attacked
	-XP pickup sound is played every second only to the cloaked player (no one else can hear)
=//

///	Inventory Slot
	LOOP {0 8 + 1}:
		@a[m=2,RRci=0] RRsis = |0| {SelectedItemSlot:|0|}
	
///	Setting tags for holding cloak + restock
	@a[RRsa=1,m=2,RRci=0] RRhi = 1 {Inventory:[{Slot:-106b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Cloaking Device"}}}]}
	@a[RRsa=1,m=2,RRci=0] RRhi = 2 {Inventory:[{Slot:-106b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Restocks Ammo"}}}]}
	@a[RRsa=1,m=2,RRci=0] RRhi = 1 {SelectedItem:{id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Cloaking Device"}}}}
	@a[RRsa=1,m=2,RRci=0] RRhi = 2 {SelectedItem:{id:"minecraft:carrot_on_a_stick",Count:1b,tag:{display:{Name:"Restocks Ammo"}}}}
	
///	Inventory save if holding cloak
	@a[RRsa=1,m=2,RRci=0,RRhi=1] ScOP @a[c=1,r=1,m=2,RRci=0,RRhi=1] RRsia = @a[c=1,r=1,m=2,RRci=0,RRhi=1] RRsis
	
///	Startup timer, only sets state to 1 if holding CoS, state = 0, class = stealth, and used CoS
///	Also including playsound
	@a[RRsa=1,m=2,RRci=0,RRhi=1,RRcs>=1,RRsst=0] particle explode ~ ~1 ~ 0.3 0.6 0.3 0 30 force @a
	@a[RRsa=1,m=2,RRci=0,RRhi=1,RRcs>=1,RRsst=0] playsound minecraft:entity.arrow.shoot voice @a[r=10] ~ ~ ~ 2 0.5
	@a[RRsa=1,m=2,RRci=0,RRhi=1,RRcs>=1,RRsst=0] RRsst = 1

///	Decrease and increase
	@a[RRsa=1,m=2,RRci=0,RRsst=1,RRsam>=1<=$StealthCloakMax$] RRsam - $StealthCloakRemove$
	@a[RRsa=1,m=2,RRci=0,RRsst=0,RRsam>=0<$StealthCloakMax$] RRsam + $StealthCloakRegen$
	@a[RRsa=1,m=2,RRci=0,RRsam>$StealthCloakMax$] RRsam = $StealthCloakMax$
	@a[RRsa=1,m=2,RRci=0,RRsam<=-1] RRsam = 0
	
///	Change (detection of changed inventory slot)
	@a[RRsa=1,m=2,RRci=0,RRsst=1] ScOP @a[c=1,r=1,RRsa=1,m=2,RRci=0,RRsst=1] RRsic = @a[c=1,r=1,RRsa=1,m=2,RRci=0,RRsst=1] RRsia
	@a[RRsa=1,m=2,RRci=0,RRsst=1] ScOP @a[c=1,r=1,RRsa=1,m=2,RRci=0,RRsst=1] RRsic -= @a[c=1,r=1,RRsa=1,m=2,RRci=0,RRsst=1] RRsis
	@a[RRsa=1,m=2,RRci=0,RRsst=1,RRsic<=-1] RRsst = 2
	@a[RRsa=1,m=2,RRci=0,RRsst=1,RRsic>=1] RRsst = 2

///	Change (detection of running out)
	@a[RRsa=1,m=2,RRci=0,RRsst=1,RRsam=0] RRsst = 2
	
///	Ending stage, including giving items back <-- Moved to under Big Earner effect
///	Starts at 0, ends at 20
	@a[RRsa=1,m=2,RRci=0,RRsst=2,RRsed>=0<=19] RRsed + 1
	@a[RRsa=1,m=2,RRci=0,RRsst=2,RRsed=20] + RRStealthInvRestore
	
	@a[RRsa=1,m=2,RRci=0,RRsst=2,RRsed=20] RRsst = 0
	@a[RRsa=1,m=2,RRci=0,RRsst=0,RRsed=20] RRsed = 0
	
///	Effects, clears
	@a[RRsa=1,m=2,RRci=0,RRsst=2] particle smoke ~ ~1 ~ 0.3 0.5 0.3 0 10 force @a
	@a[RRsa=1,m=2,RRci=0,RRsst=2] playsound minecraft:entity.squid.death voice @a[r=10] ~ ~ ~ 0.6 0.5
	clear @a[RRsa=1,m=2,RRci=0,RRsst>=1<=2]
	effect @a[RRsa=1,m=2,RRci=0,RRsst>=1<=2] 14 2 0 true
	effect @a[RRsa=1,m=2,RRci=0,RRsst=0] 14 0 0 true
	
///	XP Start calc
	@a[RRsa=1,m=2,RRci=0,RRsam>=0] ScOP @a[c=1,r=1,m=2,RRci=0,RRsam>=0] RRxp = @a[c=1,r=1,m=2,RRci=0,RRsam>=0] RRsam

//=
	@e[type=AreaEffectCloud] + RRPoisonCloud {Effects:[{Ambient:0b,ShowParticles:1b,Duration:0,Id:19b,Amplifier:10b}]}
	@e[type=AreaEffectCloud,RRPoisonCloud] RRspo + 0
	entitydata @e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] {Radius:$StealthPoisonRadius$,RadiusPerTick:0f,Duration:200,ReapplicationDelay:10000,RadiusOnUse:0f,WaitTime:0}
	@e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] + RRPoisonCloudG {Effects:[{Ambient:0b,ShowParticles:1b,Duration:0,Id:19b,Amplifier:11b}]}
	@e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] + RRPoisonCloudB {Effects:[{Ambient:0b,ShowParticles:1b,Duration:0,Id:19b,Amplifier:12b}]}
	@e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] RRspo = 1
=//
	
	@e[type=AreaEffectCloud] + RRPoisonCloudG {Effects:[{Ambient:0b,ShowParticles:1b,Duration:0,Id:19b,Amplifier:11b}]}
	@e[type=AreaEffectCloud] + RRPoisonCloudB {Effects:[{Ambient:0b,ShowParticles:1b,Duration:0,Id:19b,Amplifier:12b}]}
	
	@e[type=AreaEffectCloud,RRPoisonCloudG] + RRPoisonCloud
	@e[type=AreaEffectCloud,RRPoisonCloudB] + RRPoisonCloud
	@e[type=AreaEffectCloud,RRPoisonCloud] RRspo + 0
	entitydata @e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] {Radius:$StealthPoisonRadius$,RadiusPerTick:0f,Duration:200,ReapplicationDelay:10000,RadiusOnUse:0f,WaitTime:0}
	@e[type=AreaEffectCloud,RRPoisonCloud,RRspo=0] RRspo = 1
	
///	Poison effects
///	Cannot use poison anywhere else
///	Make it poison pigs too
	@e[type=AreaEffectCloud,RRPoisonCloudG] @a[m=2,r=$StealthPoisonRadius$,team=RRb] + RRPoisoned
	@e[type=AreaEffectCloud,RRPoisonCloudG] @e[r=$StealthPoisonRadius$,type=Pig,RRPigBlue,RRens=3] + RRPoisoned
	@e[type=AreaEffectCloud,RRPoisonCloudB] @a[m=2,r=$StealthPoisonRadius$,team=RRg] + RRPoisoned
	@e[type=AreaEffectCloud,RRPoisonCloudB] @e[r=$StealthPoisonRadius$,type=Pig,RRPigGreen,RRens=3] + RRPoisoned
	effect @a[RRsa=1,m=2,RRPoisoned] 19 1 10 true
	effect @e[type=Pig,RRPoisoned,RRens=3] 19 1 10 true
	effect @a[RRsa=1,m=2,!RRPoisoned] 19 0 10 true
	effect @e[type=Pig,!RRPoisoned,RRens=3] 19 0 10 true
	@e[RRPoisoned] - RRPoisoned
	@e[type=AreaEffectCloud,RRPoisonCloudG] particle mobSpell ~ ~ ~ 0.3 1 0.3 1 0 force
	@e[type=AreaEffectCloud,RRPoisonCloudB] particle mobSpell ~ ~ ~ 0.3 0.3 1 1 0 force
	
///	Jump boost
	@a[RRsa=1,m=2,RRci=0] + RRJumpBoost {SelectedItem:{id:"minecraft:dye",Count:1b,Damage:10s,tag:{display:{Name:"Jump Boost"}}}}
	@a[RRsa=1,m=2,RRci=0] + RRJumpBoost {Inventory:[{Slot:-106b,id:"minecraft:dye",Count:1b,Damage:10s,tag:{display:{Name:"Jump Boost"}}}]}
	effect @a[RRsa=1,RRci=0,m=2,!RRJumpBoost] 8 0 0 true
	effect @a[RRsa=1,RRci=0,m=2,RRJumpBoost] 8 1 2 true
	@a[RRJumpBoost] - RRJumpBoost

///	Big Earner Effect
///	When holding the sword: no weakness
	@a[RRci=0,RRuk=0] + RRHoldingBigEarner {SelectedItem:{id:"minecraft:golden_sword",tag:{display:{Name:"Big Earner"}}}}
	@a[RRci=0,RRuk=0,!RRHoldingBigEarner] + RRWeakness
	@a[RRHoldingBigEarner] - RRHoldingBigEarner

///	Detects when: engineer is within a 5 block radius and died, and the stealth person has killcount of 1 + usedsword of 1
	@a[RRci=3,RRde=1] @a[RRci=0,r=5,RRki=1,RRuk=1,c=1] + RRBigEarnerBuff
	@a[RRsa=1,RRci=0,RRBigEarnerBuff] RRsam + $StealthKnifeCloak$
	@a[RRsa=1,RRci=0,RRBigEarnerBuff,RRsam>=$StealthCloakMax$] RRsam = $StealthCloakMax$
	effect @a[RRsa=1,RRci=0,RRBigEarnerBuff] 1 $StealthKnifeSpeedDuration$ $StealthKnifeSpeedLevel$ true
	@a[RRuk=1] RRuk = 2
	@a[RRBigEarnerBuff] playsound Snd_Level voice @a[RRBigEarnerBuff,c=1] ~ ~ ~ 2
	@a[RRBigEarnerBuff] - RRBigEarnerBuff

///	Restocking effect
///	Takes 6s, cannot be canceled, cannot attack while doing so
///	During restocking: blindness, cleared inventory and cannot attack
	@a[RRsa=1,m=2,RRci=0,RRcs>=1,RRuk=2,RRsrt=-1,RRhi=2] RRsrt = $StealthRestockDelay$
	@a[RRsa=1,m=2,RRci=0,RRcs>=1,RRup=1,RRsrt=-1,RRhi=2] RRsrt = $StealthRestockDelay$
	@a[RRsa=1,m=2,RRci=0,RRsrt=$StealthRestockDelay$] playsound minecraft:block.chest.open voice @a[r=5] ~ ~ ~ 2
	effect @a[RRsa=1,m=2,RRci=0,RRsrt=$StealthRestockDelay$] 15 100 0 true
	effect @a[RRsa=1,m=2,RRci=0,RRsrt=$StealthRestockDelay$] 2 100 1 true
	clear @a[RRsa=1,m=2,RRci=0,RRsrt=$StealthRestockDelay$]
	
	@a[RRsa=1,m=2,RRci=0,RRsrt>=0<=$StealthRestockDelay$] RRsrt - 1
	@a[RRsa=1,m=2,RRci=0,RRsrt>=0<=$StealthRestockDelay$] particle endRod ~ ~1 ~ 0.5 0.8 0.5 0 1 force
	effect @a[RRsa=1,m=2,RRci=0,RRsrt>=$StealthRestockGlowing$] minecraft:glowing 1 0 true
	effect @a[RRsa=1,m=2,RRci=0,RRsrt<$StealthRestockGlowing$] minecraft:glowing 0 0 true
	@a[RRsa=1,m=2,RRci=0,RRsrt>=0<=$StealthRestockDelay$] ScOP @a[m=2,RRci=0,RRsrt>=1,c=1,r=1] RRxp = @a[m=2,RRci=0,RRsrt>=1,c=1,r=1] RRsrt
	
///	Canceled upon death
	@a[RRci=0,RRsrt>=1,RRde>=1] RRsrt = -1
	
///	When RRsrt score is at 0: restores inventory
	@a[RRsa=1,m=2,RRci=0,RRsrt=1] + RRStealthInvRestore
	effect @a[RRsa=1,m=2,RRci=0,RRsrt=1] 15 0 0 true
	effect @a[RRsa=1,m=2,RRci=0,RRsrt=1] 2 0 0 true
	@a[RRsa=1,m=2,RRci=0,RRsrt=1] playsound Snd_Level voice @a[c=1] ~ ~ ~ 2
	@a[RRsa=1,m=2,RRci=0,RRsrt=1] RRup = 0
	@a[RRsa=1,m=2,RRci=0,RRsrt=1] RRuk = 0
//>

//<Stealth Inventory Restore Testfor Clock
