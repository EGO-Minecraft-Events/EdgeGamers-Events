	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,team=RRg] CCItem_GlassPaneG
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,team=RRb] CCItem_GlassPaneB
	
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,RRup=0,team=RRg] slot.hotbar.4 CCItem_StealthPoisonG
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,RRup=0,team=RRb] slot.hotbar.4 CCItem_StealthPoisonB
	
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,RRur=0] CCItem_RedstoneGive(1)
	replaceitem entity @a[RRsa=1,m=2,RRStealthInvRestore,RRuk=0] slot.hotbar.5 CCItem_StealthKnife
	
	@a[RRStealthInvRestore] - RRStealthInvRestore
//>

