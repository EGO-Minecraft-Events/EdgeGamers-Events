///	food in green spawn
	LOOP {Arr_FoodPlacement[S]}:
		SPLIT {Arr_Temp |0|} { }
		summon AreaEffectCloud Arr_Temp[2] Arr_Temp[3] Arr_Temp[4] {Tags:["RRFoodTemp"],Duration:5}
		LOOP {1 Arr_Temp[0] + 1}:
			/*
			@e[type=AreaEffectCloud,RRFoodTemp] Arr_FoodPlacementRelative[|0| - 1] summon ArmorStand ~ ~ ~  
			{Tags:["RREntity","RRDisplay","RRDisplayStand"],
			DisabledSlots:2096896,NoGravity:1,Invulnerable:1,PersistenceRequired:1,Invisible:1,Rotation:[90f,0f],
			ArmorItems:[{},{},{},{id:Arr_Temp[1],Count:1b}]}
			*/
		kill @e[type=AreaEffectCloud,RRFoodTemp]
		
		UNASSIGN {ARRAY Arr_Temp}

/// animals in blue spawn
	//=
	summon Pig $SpawnAnimals$ {Tags:["RREntity","RRDisplay","RRDisplayAnimal","RRDisplayPig"],Saddle:1b,CustomName:"Porkchop",Invulnerable:1,$NoLoot$}
	summon Pig $SpawnAnimals$ {Tags:["RREntity","RRDisplay","RRDisplayAnimal","RRDisplayPig"],Saddle:1b,CustomName:"Bacon",Invulnerable:1,$NoLoot$}
	summon Pig $SpawnAnimals$ {Tags:["RREntity","RRDisplay","RRDisplayAnimal","RRDisplayPig"],Saddle:1b,CustomName:"Spider Pig",Invulnerable:1,$NoLoot$}
	=//
	summon Cow $SpawnAnimals$ {Tags:["RREntity","RRDisplay","RRDisplayAnimal","RRDisplayCow"],CustomName:"idk_moosters",Invulnerable:1,$NoLoot$}
	
	LOOP {Arr_HorsePlacement[S]}:
		SPLIT {Arr_Temp |0|} { }
		
		IF {Arr_Temp[0] = NORTH}:
			DEF GLOBAL $TempCoords$ CALC(Arr_Temp[1] + 0.5) CALC(Arr_Temp[2] - 0.9) CALC(Arr_Temp[3] + 0.5 - 0.2)
			DEF GLOBAL $TempRot$ 180f,0f

		IF {Arr_Temp[0] = SOUTH}:
			DEF GLOBAL $TempCoords$ CALC(Arr_Temp[1] + 0.5) CALC(Arr_Temp[2] - 0.9) CALC(Arr_Temp[3] + 0.5 + 0.2)
			DEF GLOBAL $TempRot$ 0f,0f

		IF {Arr_Temp[0] = EAST}:
			DEF GLOBAL $TempCoords$ CALC(Arr_Temp[1] + 0.5 + 0.2) CALC(Arr_Temp[2] - 0.9) CALC(Arr_Temp[3] + 0.5)
			DEF GLOBAL $TempRot$ -90f,0f

		IF {Arr_Temp[0] = WEST}:
			DEF GLOBAL $TempCoords$ CALC(Arr_Temp[1] + 0.5 - 0.2) CALC(Arr_Temp[2] - 0.9) CALC(Arr_Temp[3] + 0.5)
			DEF GLOBAL $TempRot$ 90f,0f
		
		/*
		summon EntityHorse $TempCoords$ 
			{Tags:["RREntity","RRDisplay","RRDisplayAnimal","RRDisplayHorse","RRDisplayInvis"],ActiveEffects:[{Id:14b,Duration:10000000,ShowParticles:0b}],
			Rotation:[$TempRot$],NoAI:1,NoGravity:1,Invulnerable:1,Silent:1,PersistenceRequired:1,$NoLoot$}
		*/
		
		UNASSIGN {DEF $TempCoords$}
		UNASSIGN {DEF $TempRot$}
		UNASSIGN {ARRAY Arr_Temp}
		
